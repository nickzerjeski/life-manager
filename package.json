    "chartjs-adapter-date-fns": "^3.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "flowbite": "^3.1.2",
    "genkit": "^1.0.4",
    "lucide-react": "^0.514.0",
    "pdf-parse": "^1.1.1",
    "react-chartjs-2": "^5.3.0",
    "react-hook-form": "^7.54.2",
    "react-markdown": "^9.1.0",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "tailwindcss-animate": "^1.0.7",
    "unified": "^11.0.5",
    "vfile": "^6.0.3",
    "vfile-message": "^4.0.2"
