    "@vitejs/plugin-react": "^4.2.0",
    "concurrently": "^9.1.2",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "genkit-cli": "^0.0.2",
    "install": "^0.13.0",
    "minimatch": "^10.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwind-merge": "^3.3.1",
