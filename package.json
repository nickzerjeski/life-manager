{
  "name": "lifemanager",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite app --config app/vite.config.mts",
    "genkit:dev": "genkit start -- tsx server/src/ai/dev.ts",
    "genkit:watch": "genkit start -- tsx --watch server/src/ai/dev.ts",
    "build": "vite build app --config app/vite.config.mts",
    "start": "vite preview app --config app/vite.config.mts",
    "lint": "ESLINT_USE_FLAT_CONFIG=false eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit -p app/tsconfig.json && tsc --noEmit -p server/tsconfig.json",
    "test": "tsx --test",
    "server": "tsx --tsconfig server/tsconfig.json server/src/server.ts"
  },
